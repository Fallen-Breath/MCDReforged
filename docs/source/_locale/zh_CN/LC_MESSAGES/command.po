msgid "!!MCDR reload plugin"
msgstr "!!MCDR reload plugin"

#: ../../source/command.rst:54
msgid "!!MCDR r plg"
msgstr "!!MCDR r plg"

#: ../../source/command.rst:55
msgid "Reload all **changed** plugins"
msgstr "重载所有 **已更改** 插件"

#: ../../source/command.rst:56
msgid "!!MCDR reload config"
msgstr "!!MCDR reload config"

#: ../../source/command.rst:57
msgid "!!MCDR r cfg"
msgstr "!!MCDR r cfg"

#: ../../source/command.rst:58
msgid "Reload config file"
msgstr "重新载入配置文件"

#: ../../source/command.rst:59
msgid "!!MCDR reload permission"
msgstr "!!MCDR reload permission"

#: ../../source/command.rst:60
